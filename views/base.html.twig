        <!-- Header -->
        <header id="page-top">
            <nav id="mainNav" class="navbar navbar-default navbar-custom navbar-fixed-top">
                <div class="container">
                    <!--<div class="container">-->
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="navbar-header page-scroll">
                        <div class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                            <span class="sr-only">Toggle navigation</span>
                            <i class="fa fa-2x fa-bars"></i>
                            <br>
                        </div>
                        <a class="navbar-brand page-scroll hidden-xs" href="#page-top">
                            <img id="mainLogo" class="brand-logo" src="assets/img/#logo">
                            <br><br>
                        </a>
                        <br><br>
                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a class="page-scroll" href="#menu1">menu1</a>
                            </li>
                        </ul>

                    </div>
                    <!-- /.navbar-collapse -->
                    <!--</div>-->
                    <!-- /.container-fluid -->
                </div>
            </nav>
            <div class="hidden-xs">
                <div id="canvasHolder"></div>
                <div class="container-fluid cearo ">
                    <div class="header-logo">
                        <div class="row">
                            <div class="col-xs-12 text-center">
                                <h1 class="text-header">[ Title ]</h1>
                                <h1 class="text-header">[ Sub-title ]</h1>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
